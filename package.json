{
  "name": "ipc-bot",
  "version": "1.0.0",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch --env-file=.env.local src/index.ts",
    "build": "npm run clean && tsc --declarationMap false --sourceMap false --declaration false && npx esbuild dist/index.js --bundle --outfile=bin/index.mjs --platform=node --target=esnext --format=esm --banner:js=\"import {createRequire} from 'module';const require=createRequire(import.meta.url);\"",
    "start": "node --env-file=.env bin/index.mjs",
    "clean": "rimraf dist && rimraf bin",
    "dev:test-socket": "npx http-server -p 3001 ./util"
  },
  "engines": {
    "node": ">=23.0.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.14.1",
    "esbuild": "^0.25.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@fastify/cors": "^11.0.1",
    "@fastify/type-provider-json-schema-to-ts": "^5.0.0",
    "discord-api-types": "^0.37.120",
    "discord.js": "^14.18.0",
    "eventemitter3": "^5.0.1",
    "fastify": "^5.3.2",
    "fastify-plugin": "^5.0.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.13.2",
    "rethinkdb-ts": "^2.7.0",
    "socket.io": "^4.8.1"
  }
}
